generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin-arm64", "linux-musl", "debian-openssl-1.1.x"]
}


datasource db {
provider = "postgresql"
url = env("DATABASE_URL")
}


model User {
id String @id @default(uuid())
email String @unique
passwordHash String
createdAt DateTime @default(now())
resetToken String? @db.Text
resetTokenExpiry DateTime?
}